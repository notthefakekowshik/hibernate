package mappingDemo;

import jakarta.persistence.*;

@Entity
public class Student {
	@Id
	private int id;
	private String name;
	private Laptop laptop;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}
